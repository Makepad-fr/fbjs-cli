{
  "name": "fbjs-cli",
  "description": "A CLI tool to automate Facebook interactions",
  "version": "0.1.0-beta",
  "author": "Makepad Developers <dev@makepad.fr>",
  "bin": {
    "fb": "./bin/run"
  },
  "bugs": "https://github.com/[object Object]/issues",
  "dependencies": {
    "@makepad/fbjs": "^4.0.0",
    "@oclif/command": "^1.8.0",
    "@oclif/config": "^1.17.0",
    "@oclif/plugin-help": "^3.2.2",
    "@types/configstore": "^5.0.0",
    "@types/inquirer": "^7.3.1",
    "conf": "^10.0.1",
    "inquirer": "^6.5.2",
    "tslib": "^1.14.1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.26.0",
    "@oclif/test": "^1.2.8",
    "@types/chai": "^4.2.18",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.60",
    "chai": "^4.3.4",
    "eslint": "^5.16.0",
    "eslint-config-oclif": "^3.1.0",
    "eslint-config-oclif-typescript": "^0.1.0",
    "globby": "^10.0.2",
    "mocha": "^8.4.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.9"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/[object Object]",
  "keywords": [
    "Automation",
    "CLI",
    "Facebook",
    "Scraping",
    "TypeScript"
  ],
  "license": "GPL-3.0-or-later",
  "main": "dist/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "fb",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Makepad-fr/fbjs-cli.git"
  },
  "scripts": {
    "compile": "node_modules/.bin/tsc ",
    "main": "dist/index.js",
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "eslint . --ext .ts --config .eslintrc",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "prepublish": "npm run compile",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
